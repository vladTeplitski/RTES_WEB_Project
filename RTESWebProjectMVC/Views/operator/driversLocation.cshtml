<!--Start Driver Locations window-->
<div id="mainDriversLocationContainer">
    <div id="DriversLocationContainer" class="infoTableClass opRepLabelJs" style="display:@ViewBag.showDriversLocationWindow;width:450px;margin-left:-225px;">
        <div class="reportLabelHead">
            <b><i Style="font-size:15px;margin-right:5px;" class="glyphicon glyphicon-record"></i>Drivers Location</b>
            <div onclick="closeViewDriversLocation()" class="x">x</div>
        </div>
        
        <div class="tableContPartial">
            <table id="myTable" class="table table - striped tablePartial" style="font-size:small;">

                <tr>
                    <th></th>
                    <th>Driver</th>
                    <th>Location</th>
                </tr>


                @{

                    if (ViewBag.driverLocationList != null)
                    {
                        foreach (var item in ViewBag.driverLocationList)
                        {

                            <tr id="updatePannelAngular" style="background-color:#5C91BF;">
                                <td>
                                    <i Style="color:#35F643;font-size:10px;margin-right:5px;" class="glyphicon glyphicon-record"></i>
                                </td>
                                <td>
                                    
                                </td>
                                <td>


                                </td>
                            </tr>

                        }
                    }
                    else
                    {
                        <tr id="updatePannelAngular">
                            <td></td>
                            <td>No info</td>
                            <td>No info</td>
                        </tr>
                    }
                }
            </table>
        </div>
    </div>
</div>
<!--END Driver Locations window-->
