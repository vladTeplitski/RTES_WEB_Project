
<div class="row row-centered " style="margin-bottom: 25px;" ng-controller="emergFormController" id="shay">


    <div class="col-lg-6 col-lg-offset-3 col-md-8 col-md-offset-2 col-xs-12 col-centered " style="background: rgba(78, 134, 183, 0.8); border-left: 5px solid #C9EC25; border-right: 5px solid #C9EC25; border-radius: 5px; margin-top: 5px; margin-bottom: 5px; padding-bottom: 5px;">
        <br />
        <div id="reportsInSystem" class="personalDetailsClass" style="width:80%;margin:auto;text-align:center">
            <i Style="font-size:20px;color:#D44E3C" class="fa fa-heartbeat"></i>
            <span style="margin-left:10px;font-size:15px;margin-top:10px;"><b>New Emergency Form</b></span>
        </div><br />
@using (Html.BeginForm("send_Details_Func", "client"))
{

        <form style="color: #ffffff" name="myForm" id="loForm">
          
            <!--personal details button-->
            
            <div ng-click="myPersonalDetails()" id="personalDtlsBtn" class="emergFormBtn">
                <i Style="font-size:23px;" class="fa fa-info-circle"></i>
                Personal Details
            </div>

            <div class="container" ng-show="personal">

                <!--Personal details content -->
                <div class="table-responsive TableDetailsCover">
                    <table class="table" id="TableDetails">

                        <tr class="colT">
                            <th>Car Category:</th>
                            <td class="colD">
                                @ViewBag.carCategory
                            </td>
                        </tr>
                        <tr class="colT">
                            <th>Car Model:</th>
                            <td class="colD">
                                @ViewBag.carModel
                            </td>
                        </tr>
                        <tr class="colT">
                            <th>Year Of Manufacture:</th>
                            <td class="colD">
                                @ViewBag.YearOfManuFacture
                            </td>
                        </tr>
                        <tr class="colT">
                            <th>License Plate:</th>
                            <td class="colD">
                                @ViewBag.liceansePlate
                            </td>
                        </tr>
                        <tr class="colT">
                            <th>Insurance Company Name:</th>
                            <td class="colD">
                                @ViewBag.InsuranceCompanyName
                            </td>
                        </tr>
                        <tr class="colT">
                            <th>Insurance Agent Name:</th>
                            <td class="colD">
                                @ViewBag.InsuranceAgentName
                            </td>
                        </tr>
                        <tr class="colT">
                            <th>Insurance Agent Phone:</th>
                            <td class="colD">
                                @ViewBag.InsuranceAgentPhone
                            </td>
                        </tr>
                        <tr class="colT">
                            <th>Insurance Policy Number:</th>
                            <td class="colD">
                                @ViewBag.InsurancePolicyNumber
                            </td>
                        </tr>
                        <tr class="colT">
                            <th>Policy Expiration:</th>
                            <td class="colD">
                                @ViewBag.PolicyExpiration
                            </td>
                        </tr>
                        <tr class="colT">
                            <th>Driving License Number:</th>
                            <td class="colD">
                                @ViewBag.DrivingLicenseNumber
                            </td>
                        </tr>

                        <tr class="colT">
                            <th>Car Owner Name:</th>
                            <td class="colD">
                                @ViewBag.CarOwnerName
                            </td>
                        </tr>
                        <tr class="colT">
                            <th>Car Owner ID:</th>
                            <td class="colD">
                                @ViewBag.CarOwnerID
                            </td>
                        </tr>

                    </table>
                </div>
            </div>
          <!--Relevant real time information -->
            <div id="Div1" class="personalDetailsClass" style="margin-top:20px; border-color:white;">

                <div style="margin-left:10px;font-size:15px;margin-top:10px;">

                    <div class="rowForm">
                        <label for="text" ID="date">Date: @ViewBag.date1</label>
                    </div>

                    <div class="rowForm">
                        <label for="text" ID="hour">Hour: @ViewBag.hour1</label>
                    </div>

                    <div class="rowForm1">
                        <label for="text" ID="location">Location:</label>
                        <div class="formContainer" style="margin-left:2px;">
                            <input ng-model="location1" class="form-control" name="location" id="location1" type="text" style="width:80%;display:inline;color:black;" placeholder="Please enter your location">
                           <span ng-hide="!location1.length" ng-hide="validName" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                            <span ng-show="!location1.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                        </div>

                        <label for="text">Towing destination:</label>

                        <br />
                        <div class="formContainer" style="margin-left:2px;">
                            <input class="form-control" name="towDest" ng-model="towDest" type="text" id="towDest" style="width:80%;display: inline;color:black;" placeholder="Enter Towing Destination" TextMode="SingleLine">
                            <span ng-hide="!towDest.length" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                            <span ng-show="!towDest.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                        </div>

                        <label for="text">Witness Name:</label>

                        <br />
                        <div class="formContainer" style="margin-left:2px;">
                            <input class="form-control" name="witName" ng-model="witName" type="text" id="witName" style="width:80%;display: inline;color:black;" placeholder="Enter Witness Name" TextMode="SingleLine">
                            <span ng-hide="!witName.length" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                            <span ng-show="!witName.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                        </div>


                        <label for="text">Witness Phone Number:</label>
                        <div class="formContainer" style="margin-left:2px;">
                            <input ng-model="witPhone" name="witPhone" class="form-control" type="text" id="witPhone" style="width:80%;display:inline;color:black;" placeholder="Enter Witness Phone Number" maxlength="7" ng-pattern="onlyNumbers" required>
                            <span ng-show="myForm.witPhone.$error.pattern && witPhone.length" Style="color:red;padding-left:5px;" class="glyphicon glyphicon-remove form-control-feedback"></span>
                            <span ng-hide="!witPhone.length"  Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                            <span ng-show="!witPhone.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                            <div class="error" ng-show="myForm.witPhone.$dirty"><br /> </div>
                            <span class="errorFields" ng-show="myForm.witPhone.$invalid && !myForm.witPhone.$pristine && myForm.witPhone.$error.pattern"> Only numbers are allowed</span>
                        </div>

                    </div>

                   
                    <label for="text">Comments:</label>
                    <textarea id="TextArea1" name="comments" class="form-control" rows="5" cols="40" placeholder="Comments here..." style="color: #000000;width:auto;">  </textarea>
                    <br />
                </div>
            </div><br />
        

            <!-- <div class="checkbox checkbox-info emergFormBtn">

                 <input id="checkbox4" type="checkbox"  ng-click="checkboxfunc()" ng-model="checked">
                 <label for="checkbox4">
                     Third Party Chapter
                 </label>
             </div>-->
            <!--Start Third party details -->
            <div ng-click="thirdPartyDetails()" id="addThirdPartyDtls" class="emergFormBtn">
                <i Style="font-size:20px;margin-right:10px;" class="glyphicon glyphicon-th-list"></i>
                Fill Third Party Details
            </div>
            <br />

            <!--Start Driver details -->
            <div ng-click="myFunc()" id="driverDtls" class="emergFormBtn" class="driverDetailsButt" ng-show="driverDetailsBut">
                <i Style="font-size:20px;margin-right:10px;" class="glyphicon glyphicon-user"></i>
                Driver Details
            </div>

            <br />

            <!--slide down driver details div-->
            <div style="margin-bottom: 20px;width:90%;" class="driverDetails" ng-show="showMe">
                <label for="text">Please enter the third part details:</label>
                <br />
                <label for="text">Driver Name:</label>

                <br />
                <div class="formContainer">
                    <input id="driver_name" type="text" ng-model="myName" class="form-control" name="myName" style="display: inline;color:black;" placeholder="Enter Driver Name" ng-pattern="onlyLetters" required>
                    <span ng-hide="!myName.length " Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="myForm.myName.$error.pattern" Style="color:red;padding-left:5px;" class="glyphicon glyphicon-remove form-control-feedback"></span>
                    <span ng-show="!myName.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.myName.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.myName.$invalid && !myForm.myName.$pristine && myForm.myName.$error.pattern"> Only Alphabets are allowed</span>
                </div>

                <label for="text">Driver ID:</label>
                <div class="formContainer">
                    <input ng-model="driverID" name="driverID" class="form-control" type="text" id="driver_ID" style="display:inline;color:black;" placeholder="Enter Driver ID" maxlength="9" ng-pattern="onlyNumbers" required>
                    <span ng-show="myForm.driverID.$error.pattern && driverID.length" Style="color:red;padding-left:5px;" class="glyphicon glyphicon-remove form-control-feedback"></span>
                    <span ng-hide="!driverID.length" ng-hide="driverIdSuccess" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!driverID.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.driverID.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.driverID.$invalid && !myForm.driverID.$pristine && myForm.driverID.$error.pattern"> Only numbers are allowed</span>
                </div>

                <label for="text">Phone Number:</label>
                <div class="formContainer">
                    <input ng-model="driverPhone" name="driverPhone" class="form-control" type="text" id="phoneNum" style="display:inline;color:black;" placeholder="Enter Phone Number" maxlength="10" ng-pattern="onlyNumbers" required>
                    <span ng-hide="!driverPhone.length" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!driverPhone.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.driverPhone.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.driverPhone.$invalid && !myForm.driverPhone.$pristine && myForm.driverPhone.$error.pattern"> Only numbers are allowed</span>
                </div>

                <label for="text">Driving License Number:</label>
                <div class="formContainer">
                    <input ng-model="driverLicenseNum" name="driverLicenseNum" class="form-control" type="text" id="licenseNum" style="display:inline;color:black;" placeholder="Enter Driving License Number" maxlength="7" ng-pattern="onlyNumbers" required>
                    <span ng-hide="!driverLicenseNum.length" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!driverLicenseNum.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.driverLicenseNum.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.driverLicenseNum.$invalid && !myForm.driverLicenseNum.$pristine && myForm.driverLicenseNum.$error.pattern"> Only numbers are allowed</span>
                </div>

                <label for="text">Address:  </label>
                <div class="formContainer">
                    <input ng-model="driverAddress" class="form-control" type="text" name="address1" id="address1" style="display:inline;color:black;" placeholder="Enter Address" />
                    <span ng-hide="!driverAddress.length" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!driverAddress.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                </div>


            </div>


            <!--Start car details-->

            <div ng-click="myFunc1()" id="carDtls" class="emergFormBtn carDetailsButt" ng-show="carDetailsBut">
                <i Style="font-size:20px;margin-right:10px;" class="fa fa-car"></i>
                Car Details
            </div>

            <!--slide down car details-->
            <div style="margin-top: 20px;" class="carDetails" ng-show="myValue">

                <label for="text">Car Owner Name:</label>
                <div class="formContainer">
                    <input ng-model="carOwnerName" name="carOwnerName" class="form-control" type="text" id="car_ownerName" style="width:90%;display:inline;color:black;" placeholder="Enter Car Owner Name" ng-pattern="onlyLetters" required>
                    <span ng-hide="!carOwnerName.length" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!carOwnerName.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.myName.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.carOwnerName.$invalid && !myForm.carOwnerName.$pristine && myForm.carOwnerName.$error.pattern"> Only Alphabets are allowed</span>
                </div>

                <label for="text">Car Owner ID:</label>
                <div class="formContainer">
                    <input ng-model="carOwnerId" name="carOwnerId" class="form-control" type="text" id="car_ownerID" style="width:90%;display:inline;color:black;" placeholder="Enter Car Owner ID" ng-pattern="onlyNumbers" required>
                    <span ng-hide="!carOwnerId.length" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!carOwnerId.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.myName.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.carOwnerId.$invalid && !myForm.carOwnerId.$pristine && myForm.carOwnerId.$error.pattern"> Only numbers are allowed</span>
                </div>

                <label for="text">License Plate Number:</label>
                <div class="formContainer">
                    <input ng-model="carLicensePlate" name="carLicensePlate" class="form-control" type="text" id="license_plateNum" style="width:90%;display:inline;color:black;" placeholder="Enter License Plate Number" ng-pattern="onlyNumbers" required>
                    <span ng-hide="!carLicensePlate.length" ng-hide="validName" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!carLicensePlate.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.myName.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.carLicensePlate.$invalid && !myForm.carLicensePlate.$pristine && myForm.carLicensePlate.$error.pattern"> Only numbers are allowed</span>
                </div>

                <label for="text">Car Category:</label>
                <div class="formContainer">
                    <input ng-model="carCategory" name="carCategory" class="form-control" type="text" id="carCategory" style="width:90%;display:inline;color:black;" placeholder="Enter Car Category" ng-pattern="onlyLetters" required>
                    <span ng-hide="!carCategory.length" ng-hide="validName" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!carCategory.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.myName.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.carCategory.$invalid && !myForm.carCategory.$pristine && myForm.carCategory.$error.pattern"> Only Alphabets are allowed</span>
                </div>

                <label for="text">Car Model:</label>
                <div class="formContainer">
                    <input ng-model="carModel" name="carModel" class="form-control" type="text" id="carModel" style="width:90%;display:inline;color:black;" placeholder="Enter Car Model" ng-pattern="onlyLetters" required>
                    <span ng-hide="!carModel.length" ng-hide="validName" Style="color:#057B28;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!carModel.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.myName.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.carModel.$invalid && !myForm.carModel.$pristine && myForm.carModel.$error.pattern"> Only Alphabets are allowed</span>
                </div>

                <label for="text">Car Color:</label>
                <select id="color1" name="color1" class="selectpicker show-tick">
                    <option>Red</option>
                    <option>White</option>
                    <option>Blue</option>
                    <option>Silver</option>
                    <option>Green</option>
                    <option>Purple</option>
                    <option>Brown</option>
                    <option>Yellow</option>
                    <option>Black</option>
                    <option>Grey</option>
                </select>

                <br />

                <label for="text">Year Of Manu facture:</label>

                <input class="form-control" type="number" name="year" id="year" min="1995" max="2017" style="color:black;width: 48%;padding-left:50px;" placeholder="Year Of Manu facture" />
            </div>
            <br />
         
        
            <!--Start Insurance details-->
           <div ng-click="myFunc2()" id="insCompDtls" class="emergFormBtn insuranceDetailsButt" ng-show="insuranceDetailsBut">
                <i Style="font-size:20px;margin-right:10px;" class="fa fa-bank"></i>
                Insurence Company Details
            </div>

            <br />
            <!--slide down insurance company details div-->
            <div class="insuranceDetails" ng-show="myValue1">

                <label for="text">Insurance Company Name:</label>
                <div class="formContainer">
                    <input ng-model="compName" name="compName" class="form-control" type="text" id="insuranceCompanyN" style="width:80%;display:inline;color:black;" placeholder="Enter Insurance Company Name" ng-pattern="onlyLetters" required>
                    <span ng-hide="!compName.length" ng-hide="validName" Style="color:lightgreen;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!compName.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.myName.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.compName.$invalid && !myForm.compName.$pristine && myForm.compName.$error.pattern"> Only Alphabets are allowed</span>
                </div>

                <label for="text">Insurance Policy Number:</label>
                <div class="formContainer">
                    <input ng-model="policyNum" name="policyNum" class="form-control" type="text" id="insurancePolicyNum" style="width:80%;display:inline;color:black;" placeholder="Enter Insurance Policy Number" ng-pattern="onlyNumbers" required>
                    <span ng-hide="!policyNum.length" ng-hide="validName" Style="color:lightgreen;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!policyNum.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.myName.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.policyNum.$invalid && !myForm.policyNum.$pristine && myForm.policyNum.$error.pattern"> Only numbers are allowed</span>
                </div>

                <label for="text">Insurance Agent Name:</label>
                <div class="formContainer">
                    <input ng-model="agentName" name="agentName" class="form-control" id="insuranceAgentNa" type="text" style="width:80%;display:inline;color:black;" placeholder="Enter Insurance Agent Name" ng-pattern="onlyLetters" required>
                    <span ng-hide="!agentName.length" ng-hide="validName" Style="color:lightgreen;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!agentName.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.myName.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.agentName.$invalid && !myForm.agentName.$pristine && myForm.agentName.$error.pattern"> Only Alphabets are allowed</span>
                </div>

                <label for="text">Insurance Agent Phone Number:</label>
                <div class="formContainer">
                    <input ng-model="agentPhone" name="agentPhone"  class="form-control" type="text" id="insuranceAgentPhone" style="width:80%;display:inline;color:black;" placeholder="Enter Insurance Agent Phone Number" ng-pattern="onlyNumbers" required>
                    <span ng-hide="!agentPhone.length" ng-hide="validName" Style="color:lightgreen;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-ok form-control-feedback"></span>
                    <span ng-show="!agentPhone.length" Style="color:#FFC600;padding-left:5px;position:relative;display:inline;" class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
                    <div class="error" ng-show="myForm.myName.$dirty"><br /> </div>
                    <span class="errorFields" ng-show="myForm.agentPhone.$invalid && !myForm.agentPhone.$pristine && myForm.agentPhone.$error.pattern"> Only numbers are allowed</span>
                </div>
            </div>


            <!--<div id="callTowService" class="personalDetailsClass checkbox" style="width:35%;background-color:#B1D607;">
                <i Style="font-size:20px;" class="fa fa-truck"></i>
                <span style="margin-left:10px;font-size:15px;margin-top:5px;">
                    <label>  <input type="checkbox" id="CheckBox1" Text="Call For Towing ">Call For Towing</label>
                </span>
            </div>-->
